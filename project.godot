[application]

config/name="MoonRiver"
run/main_scene="res://GameAndUI.tscn"
config/icon="res://Sprites/icon.png"
config/windows_native_icon="res://Sprites/icon.ico"

[autoload]

SceneChanger="*res://Scripts/SceneChanger.gd"
Progression="*res://Scripts/ProgressionSingleton.gd"

[display]

window/size/width=640
window/size/height=360
window/size/resizable=false
window/size/fullscreen=true
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot_icon/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/custom="res://menu_theme.tres"

